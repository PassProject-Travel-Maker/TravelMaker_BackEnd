<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ssafy.backend.domain.member.mapper.MemberMapper">
    <!-- 내 정보 조회 -->
    <select id="findById" parameterType="string" resultType="com.ssafy.backend.domain.member.dto.MemberDto$TestRespDto">
        select member_id as id, name, nickname, email, addr
        from member
        where member_id=#{id}
    </select>
</mapper>